# Bittensor Configuration
NETUID=234
SUBTENSOR_NETWORK="test"
SUBTENSOR_CHAIN_ENDPOINT="wss://test.finney.opentensor.ai:443"
WALLET_NAME="your-wallet"
HOTKEY="your-hotkey"
AXON_PORT="22181"

# Cloud Provider Selection
PROVIDER=GCP

# Generic Cloud Credentials
CLOUD_PROJECT_ID=your-tenant-id
CLOUD_AUTH_ID=your-client-id
CLOUD_AUTH_SECRET=your-client-secret
CLOUD_RESOURCE_GROUP=/subscriptions/your-subscription-id/resourceGroups/your-resource-group

# Generic Network Configuration
VPC_NAME=myVNet
SUBNET_NAME=mySubnet
VPC_CIDR=10.0.0.0/8
SUBNET_CIDR=10.0.0.0/24

# Generic Compute Configuration
REGION=eastus
VM_SIZE_SMALL=Standard_B1ms    # For Traffic Generators
VM_SIZE_LARGE=Standard_B2ms    # For King (can be different/larger)
NUM_TGENS=2

# Optional: Custom VM Specifications for King
# CUSTOM_KING_CPU_COUNT=2
# CUSTOM_KING_RAM_MB=4096

# Optional: Custom VM Specifications for TGens
# CUSTOM_TGEN_CPU_COUNT=8
# CUSTOM_TGEN_RAM_MB=16384
